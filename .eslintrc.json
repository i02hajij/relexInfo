{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "object-curly-spacing": "off",
        "max-len":"off",
        "indent": "off",
        "linebreak-style": "off",
        "comma-dangle": "off",
        "new-cap": "off",
        "space-before-function-paren":"off"
    }
}
